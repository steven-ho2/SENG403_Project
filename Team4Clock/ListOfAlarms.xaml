<UserControl x:Class="Team4Clock.ListOfAlarms"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Team4Clock"
             mc:Ignorable="d" 
             d:DesignHeight="350" d:DesignWidth="525">
    <Canvas Background="#FF0F90EA">
        <ScrollViewer Height="350" Width="525" VerticalScrollBarVisibility="Auto">
            <StackPanel Height="Auto" Width="525" VerticalAlignment="Stretch"/>
        </ScrollViewer>
    </Canvas>
</UserControl>
